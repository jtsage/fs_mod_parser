        <price>57500</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>NEWHOLLAND</brand>
        <category>tractorsS</category>
        <shopTranslationOffset>0 -0.219 0</shopTranslationOffset>
