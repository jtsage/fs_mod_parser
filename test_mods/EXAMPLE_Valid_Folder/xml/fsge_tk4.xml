        </movingTools>

        <movingParts>
            <movingPart node="attacherJointBackArmMain" referencePoint="attacherJointBackArmMainPartRef" localReferencePoint="attacherJointBackArmMainPart" referenceFrame="attacherJointBackArmMainRefFrame" limitedAxis="1">
                <dependentPart node="attacherJointBackArmMainPart"/>
