    <base>
        <typeDesc>$l10n_typeDesc_tractor</typeDesc>
        <filename>$data/vehicles/newHolland/tk4/tk4.i3d</filename>
        <sounds filename="$data/vehicles/rigitrac/ske50/sounds/ske50.xml" volumeFactor="0.8"/>
        <size width="2.2" length="3.8" height="2.65" lengthOffset="0.05"/>
        <components>
            <component centerOfMass="0 0.75 0.07" solverIterationCount="30" mass="1575" />
        </components>
        <schemaOverlay attacherJointPosition="0 0" name="VEHICLE" />
        <mapHotspot type="TRACTOR" />
    </base>

    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueDefault" saveId="CRAWLER">
                <wheels speedDependentRotateBack="false" autoRotateBackSpeed="5">
                    <wheel hasParticles="true" hasTireTracks="true" >
                        <physics rotSpeed="1" restLoad="0.8" repr="crawlerFrontLeft" driveNode="wheelFrontLeft"   showSteeringAngle="false" radius="0.32" width="0.3" mass="0.5" initialCompression="10" suspTravel="0.1" spring="50" damper="40" frictionScale="2.0" tireType="crawler" supportsWheelSink="false"/>
                        <tire tireTrackAtlasIndex="7"/>
                    </wheel>
                    <wheel>
                        <physics rotSpeed="1" restLoad="1.0" repr="wheelFrontLeft2"                               showSteeringAngle="false" radius="0.32" width="0.3" mass="0.5" initialCompression="10" suspTravel="0.1" spring="50" damper="40" frictionScale="2.0" tireType="crawler" supportsWheelSink="false"/>
                    </wheel>
                    <wheel hasParticles="true" hasTireTracks="true" >
                        <physics rotSpeed="1" restLoad="0.8" repr="crawlerFrontRight" driveNode="wheelFrontRight" showSteeringAngle="false" radius="0.32" width="0.3" mass="0.5" initialCompression="10" suspTravel="0.1" spring="50" damper="40" frictionScale="2.0" tireType="crawler" supportsWheelSink="false"/>
                        <tire tireTrackAtlasIndex="7"/>
                    </wheel>
                    <wheel>
                        <physics rotSpeed="1" restLoad="1.0" repr="wheelFrontRight2"                              showSteeringAngle="false" radius="0.32" width="0.3" mass="0.5" initialCompression="10" suspTravel="0.1" spring="50" damper="40" frictionScale="2.0" tireType="crawler" supportsWheelSink="false"/>
                    </wheel>
                </wheels>
                <crawlers>
                    <crawler filename="$data/shared/crawlers/newHolland/tk4Crawler/tk4Crawler.xml" linkNode="crawlerFrontLeftLinkNode" wheelIndices="1 2" isLeft="true" trackWidth="1"/>
                    <crawler filename="$data/shared/crawlers/newHolland/tk4Crawler/tk4Crawler.xml" linkNode="crawlerFrontRightLinkNode" wheelIndices="3 4" isLeft="false" trackWidth="1"/>
                </crawlers>
            </wheelConfiguration>
        </wheelConfigurations>

        <ackermannSteeringConfigurations>
            <ackermannSteering rotSpeed="50" rotMax="65" rotCenter="0 0"/>
        </ackermannSteeringConfigurations>
    </wheels>

    <lights>
        <states>
            <state lightTypes="0"/>
        </states>

        <realLights>
            <low>
                <light node="frontLightLow" lightTypes="0"/>
            </low>
            <high>
                <light node="frontLightHigh" lightTypes="0"/>
                <light node="backLightsHigh" lightTypes="0"/>
                <brakeLight node="backLightsHigh" />
            </high>
        </realLights>

        <defaultLights>
            <defaultLight shaderNode="frontLights_static" lightTypes="0" intensity="50"/>
            <defaultLight shaderNode="backLights_static"  lightTypes="0" intensity="200"/>
        </defaultLights>

        <brakeLights>
            <brakeLight shaderNode="backLights_static" />
        </brakeLights>

        <dashboards>
            <dashboard displayType="EMITTER" valueType="lightState" node="iconDefaultLight01_decals" intensity="0.3" lightTypes="0" groups="MOTOR_ACTIVE" baseColor="SHARED_BLACK4" emitColor="green"/>
            <dashboard displayType="EMITTER" valueType="lightState" node="iconDefaultLight02_decals" intensity="0.3" lightTypes="0" groups="MOTOR_ACTIVE" />
            <dashboard displayType="EMITTER" valueType="lightState" node="needleRPM"                                 lightTypes="0" groups="MOTOR_ACTIVE" />
            <dashboard displayType="EMITTER" valueType="lightState" node="needleTemperature"                         lightTypes="0" groups="MOTOR_ACTIVE" />
            <dashboard displayType="EMITTER" valueType="lightState" node="needleFuel"                                lightTypes="0" groups="MOTOR_ACTIVE" />
            <dashboard displayType="EMITTER" valueType="lightState" node="startingDisplay"                 intensity="0.3" lightTypes="0" groups="MOTOR_ACTIVE" />
        </dashboards>
    </lights>

    <attacherJoints comboDuration="2">
        <attacherJoint node="attacherJointBack" lowerRotationOffset="0.58" upperRotationOffset="-0.05" moveTime="2.5" comboTime="1">
            <distanceToGround lower="0.15" upper="0.51"/>
            <rotationNode node="attacherJointBackRot" upperRotation="18 0 0" lowerRotation="-15 0 0"/>
            <rotationNode2 node="attacherJointBackRot2"/>
            <bottomArm rotationNode="attacherJointBackArmBottom" translationNode="attacherJointBackArmBottomTrans" referenceNode="referencePointBackBottom" zScale="-1" startRotation="-10 0 0"/>
            <topArm baseNode="attacherJointBackArmTop" filename="$data/shared/assets/upperLinks/walterscheid01.i3d" zScale="-1" color="SHARED_DA_METAL_BLACK" />
            <schema position="1 0" rotation="0" invertX="false"/>
        </attacherJoint>

        <attacherJoint node="trailerAttacherJointBack"  jointType="trailer" lowerRotLimit="25 60 50" allowsJointLimitMovement="false" comboTime="1">
            <schema position="1 0" rotation="0" invertX="false" />
        </attacherJoint>

        <attacherJointConfigurations>
            <!-- Front Attacher -->
            <attacherJointConfiguration name="$l10n_configuration_valueDefault" price="0">
                <attacherJoint node="trailerAttacherJointFront" jointType="trailer" lowerRotLimit="25 65 50" allowsJointLimitMovement="false" comboTime="0">
                    <schema position="1 0" rotation="0" invertX="true" />
                </attacherJoint>
                <objectChange node="frontAttacher"      visibilityActive="true" />
                <objectChange node="frontWeight"        visibilityActive="false" />
                <objectChange node="frontWeightColPart" compoundChildActive="false" />
            </attacherJointConfiguration>

            <!-- Front Weight -->
            <attacherJointConfiguration name="$l10n_configuration_frontweight" price="400" params="300">
                <objectChange node="tk4_main_component1" massActive="1875" centerOfMassActive="0 0.75 0.15"/>
                <objectChange node="frontAttacher"      visibilityActive="false" />
                <objectChange node="frontWeight"        visibilityActive="true" />
                <objectChange node="frontWeightColPart" compoundChildActive="true" />
            </attacherJointConfiguration>
        </attacherJointConfigurations>
    </attacherJoints>

    <connectionHoses>
        <target attacherJointIndices="1 2" type="electric"        node="electric"        socket="electric"/>
        <target attacherJointIndices="1 2" type="hydraulicIn"     node="hydraulicIn01"   socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicOut"    node="hydraulicOut01"  socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicIn"     node="hydraulicIn02"   socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicOut"    node="hydraulicOut02"  socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicIn"     node="hydraulicIn03"   socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicOut"    node="hydraulicOut03"  socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicIn"     node="hydraulicIn04"   socket="hydraulic01"/>
        <target attacherJointIndices="1 2" type="hydraulicOut"    node="hydraulicOut04"  socket="hydraulic01"/>
    </connectionHoses>

    <powerTakeOffs>
        <output outputNode="ptoBack" attacherJointIndices="1 2"/>
    </powerTakeOffs>

    <enterable>
        <exitPoint node="exitPoint"/>
        <enterReferenceNode node="tk4_main_component1"/>

        <cameras>
            <camera node="outdoorCamera" rotatable="true" rotateNode="outdoorCameraTarget" limit="true" useWorldXZRotation="true" rotMinX="-1.4" rotMaxX="1" transMin="2.6" transMax="40" translation="0 0 4.4" rotation="-20 180 0">
                <raycastNode node="cameraRaycastNode1"/>
                <raycastNode node="cameraRaycastNode2"/>
                <raycastNode node="cameraRaycastNode3"/>
            </camera>
            <camera node="indoorCamera" rotatable="true" limit="true" rotMinX="-1.1" rotMaxX="0.4" transMin="0" transMax="0" useMirror="true" isInside="true" useOutdoorSounds="true" shadowFocusBox="shadowFocusBox" suspensionNodeIndex="1" />
        </cameras>

        <characterNode node="playerSkin" cameraMinDistance="0.8" spineRotation="-90 0 90">
            <target ikChain="leftArm"   targetNode="playerLeftHandTarget"  poseId="narrowFingers" />
            <target ikChain="rightArm"  targetNode="playerRightHandTarget" poseId="narrowFingers" />
            <target ikChain="leftFoot"  targetNode="playerLeftFootTarget" />
            <target ikChain="rightFoot" targetNode="playerRightFootTarget" />
        </characterNode>

        <characterTargetNodeModifier node="playerLeftFootTarget" transitionTime="0.1" >
            <state node="playerLeftFootTargetClutch" referenceNode="pedalClutch" directionReferenceNode="playerLeftFootTargetClutchRef" />
        </characterTargetNodeModifier>

        <characterTargetNodeModifier node="playerRightFootTarget" transitionTime="0.1" >
            <state node="playerRightFootTargetGas" referenceNode="pedalGas" directionReferenceNode="playerRightFootTargetGasRef" />
        </characterTargetNodeModifier>

        <mirrors>
            <mirror node="mirrorLeft" prio="1"/>
        </mirrors>
    </enterable>

    <suspensions>
        <suspension node="seat" weight="150" minTranslation="-0.05 -0.15 0.0" maxTranslation="0.05 0.15 0.0" suspensionParametersX="20 4" suspensionParametersY="5 1" suspensionParametersZ="20 4" />
        <suspension useCharacterTorso="true" weight="90" minRotation="0 -5 -5" maxRotation="0 5 5" suspensionParametersY="7 1" suspensionParametersZ="7 1" />
    </suspensions>

    <motorized forceSpeedHudDisplay="true">
        <consumerConfigurations>
            <consumerConfiguration consumersEmptyWarning="$l10n_warning_motorBatteryEmpty">
                <consumer fillUnitIndex="1" usage="22" fillType="electricCharge"/> <!-- usage is per hour at max load and max rpm -->
            </consumerConfiguration>
        </consumerConfigurations>

        <differentialConfigurations>
            <differentialConfiguration>
                <differentials>
                    <differential torqueRatio="0.5" maxSpeedRatio="1.5" wheelIndex1="1" wheelIndex2="2"/>
                    <differential torqueRatio="0.5" maxSpeedRatio="1.5" wheelIndex1="3" wheelIndex2="4" />
                    <differential torqueRatio="0.5" maxSpeedRatio="2.2" differentialIndex1="1" differentialIndex2="2"/>
                </differentials>
            </differentialConfiguration>
        </differentialConfigurations>

        <motorConfigurations>
           <motorConfiguration name="TK4 Electric Power" hp="75" price="0" consumerConfigurationIndex="1">
                <motor torqueScale="0.319" minRpm="850" maxRpm="2200" brakeForce="1.5" lowBrakeForceScale="0.1" maxForwardSpeed="12" maxBackwardSpeed="12" dampingRateScale="0.26">
                    <torque normRpm="0.45" torque="0.9"/>
                    <torque normRpm="0.50" torque="0.97"/>
                    <torque normRpm="0.59" torque="1"/>
                    <torque normRpm="0.72" torque="1"/>
                    <torque normRpm="0.86" torque="0.88"/>
                    <torque normRpm="1.00" torque="0.72"/>
                </motor>
                <transmission autoGearChangeTime="1" gearChangeTime="0.4" axleRatio="0.9" name="$l10n_info_transmission_manual">
                    <directionChange useGear="false" useGroup="false" changeTime="0.5"/>
                    <groups type="DEFAULT" changeTime="0.5">
                        <group ratio="0.500" name="A" />
                        <group ratio="1.000" name="B" />
                    </groups>
                    <forwardGear maxSpeed="4.00"  name="1"/>
                    <forwardGear maxSpeed="6.00"  name="2"/>
                    <forwardGear maxSpeed="9.00" name="3"/>
                    <forwardGear maxSpeed="12.00" name="4"/>
                </transmission>
            </motorConfiguration>
        </motorConfigurations>

        <gearLevers>
            <gearLever node="leverGears" centerAxis="2" changeTime="0.3" handsOnDelay="0.2">
                <state gear="1" xRot="7"  yRot="-7" />
                <state gear="2" xRot="-7" yRot="-7" />
                <state gear="3" xRot="7"  yRot="7" />
                <state gear="4" xRot="-7" yRot="7" />
            </gearLever>

            <gearLever node="leverGroups" centerAxis="2" changeTime="0.3" handsOnDelay="0.2">
                <state group="1" xRot="-10" />
                <state group="2" xRot="10" />
            </gearLever>
        </gearLevers>

        <!-- <exhaustEffects> -->
            <!-- <exhaustEffect node="exhaustParticle" filename="$data/effects/exhaust/exhaust.i3d" minRpmColor="0.9 0.9 0.9 0.1" maxRpmColor="0.9 0.9 0.9 0.4" minRpmScale="0.02" maxRpmScale="0.1" /> -->
        <!-- </exhaustEffects> -->

        <motorStartDuration>3000</motorStartDuration>

        <dashboards>
            <dashboard displayType="ANIMATION"   valueType="rpm" animName="needleRPM" doInterpolation="true" interpolationSpeed="1" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="ROT"         valueType="motorTemperature" node="needleTemperature" minRot="0" maxRot="115" rotAxis="3" doInterpolation="true" interpolationSpeed="1" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="ROT"         valueType="clutchPedal" node="pedalClutch" minRot="0" maxRot="9" rotAxis="1" doInterpolation="true" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="EMITTER"     valueType="motorTemperatureWarning" node="iconWarningTemperature_decals" intensity="0.5" baseColor="SHARED_BLACK4" emitColor="RED" warningThresholdMin="100" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="ROT"         valueType="movingDirection" node="leverDirection" minRot="20 0 0" maxRot="-20 0 0" doInterpolation="true" groups="MOTOR_ACTIVE"/>
            <dashboard displayType="MULTI_STATE" valueType="ignitionState" node="keyIgnition" doInterpolation="true" groups="MOTOR_ACTIVE">
                <state value="0" rotation="0 0 0"/>
                <state value="1" rotation="0 0 -90"/>
                <state value="2" rotation="0 0 -45"/>
            </dashboard>
            <dashboard displayType="TEXT" valueType="speed" node="numbersSpeed" textColor="0 0 0 1" emissiveScale="0.5" textMask="00.0" textSize="0.006" font="GENERIC" groups="MOTOR_RUNNING"/>
            <dashboard displayType="TEXT" valueType="rpm" node="numbersRpm" textColor="0 0 0 1" emissiveScale="0.5" textMask="0000" textSize="0.006" font="GENERIC" groups="MOTOR_RUNNING"/>
        </dashboards>
    </motorized>

    <drivable>
        <steeringWheel node="steeringWheel" indoorRotation="15" outdoorRotation="15"/>

        <dashboards>
            <dashboard displayType="ROT" valueType="accelerationAxis" node="pedalGas" minRot="0" maxRot="9" rotAxis="1" doInterpolation="true" groups="MOTOR_ACTIVE"/>
        </dashboards>
    </drivable>

    <animations>
        <animation name="needleFuel">
            <part node="needleFuel" startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 102" />
        </animation>

        <animation name="needleTemperature">
            <part node="needleTemperature" startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 116" />
        </animation>

        <animation name="needleRPM">
            <part node="needleRPM" startTime="0" endTime="22" startRot="0 0 0" endRot="0 0 -175" />
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="attacherJointBackArmBottom">
                <dependentPart node="attacherJointBackArmMain"/>
                <dependentPart node="attacherJointBackArmBottomChain01"/>
                <dependentPart node="attacherJointBackArmBottomChain02"/>
            </movingTool>

            <movingTool node="pedalClutch">
