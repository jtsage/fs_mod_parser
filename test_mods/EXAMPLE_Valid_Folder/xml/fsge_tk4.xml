            <movingPart node="pedalClutchHydraulic" referencePoint="pedalClutchHydraulicRef" referenceFrame="pedalClutchHydraulicRef" isActiveDirty="true" maxUpdateDistance="20" scaleZ="true"/>

            <movingPart node="pedalGasSpring" referencePoint="pedalGasSpringRef" referenceFrame="pedalGasSpringRef" isActiveDirty="true" maxUpdateDistance="20" scaleZ="true"/>
            <movingPart node="pedalGasHydraulic" referencePoint="pedalGasHydraulicRef" referenceFrame="pedalGasHydraulicRef" isActiveDirty="true" maxUpdateDistance="20" scaleZ="true"/>

            <movingPart node="crawlerFrontLeftLinkNode" referencePoint="wheelFrontLeft2" referenceFrame="tk4_main_component1" isActiveDirty="true" maxUpdateDistance="20"/>
            <movingPart node="crawlerFrontRightLinkNode" referencePoint="wheelFrontRight2" referenceFrame="tk4_main_component1" isActiveDirty="true" maxUpdateDistance="20"/>
        </movingParts>
    </cylindered>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits>
                    <fillUnit unitTextOverride="$l10n_unit_literShort" showOnHud="false" showInShop="false" fillTypes="electricCharge" capacity="80">
                        <dashboard displayType="ANIMATION" valueType="fillLevel" animName="needleFuel" doInterpolation="true" interpolationSpeed="1" groups="MOTOR_ACTIVE"/>
                        <dashboard displayType="EMITTER"   valueType="fillLevelWarning" node="iconWarningFuel_decals" emitColor="SHARED_YELLOW1" warningThresholdMax="20" groups="MOTOR_ACTIVE"/>
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dashboard>
        <groups>
            <group name="MOTOR_STARTING" isMotorStarting="true" isMotorRunning="false"/>
            <group name="MOTOR_RUNNING"   isMotorStarting="false" isMotorRunning="true"/>
            <group name="MOTOR_ACTIVE"   isMotorStarting="true" isMotorRunning="true"/>
            <group name="BACK_ATTACHMENT"  attacherJointIndices="1 2"/>
            <group name="FRONT_ATTACHMENT" attacherJointIndices="3"/>
        </groups>

        <default>
            <dashboard displayType="EMITTER" node="engineStartRed_decals"    intensity="0.3" baseColor="SHARED_BLACK4" emitColor="RED" groups="MOTOR_STARTING" />
            <dashboard displayType="EMITTER" node="engineStartYellow_decals" intensity="0.3" baseColor="SHARED_BLACK4" emitColor="YELLOW" groups="MOTOR_STARTING" />

            <dashboard displayType="VISIBILITY" node="startingDisplay" intensity="0.1" groups="MOTOR_STARTING" />
            <dashboard displayType="VISIBILITY" node="runningDisplay" intensity="0.1" groups="MOTOR_RUNNING" />
        </default>
    </dashboard>

