            <movingPart node="attacherJointBackArmBottomChain02" referencePoint="attacherJointBackArmBottomChain02Ref" referenceFrame="attacherJointBackArmBottomChain02Ref" isActiveDirty="true" maxUpdateDistance="100" scaleZ="true"/>

